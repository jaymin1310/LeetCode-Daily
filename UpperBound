class Solution{
public:
    int upperBound(vector<int> &nums, int x){
        int n=nums.size();
        int low=0,high=n-1,mid,ans=n;
        while(low<=high){
            mid=(low+high)/2;
            if(nums[mid]>x){
                ans=mid;
                high=mid-1;
            }
            else{
                low=mid+1;
            }
        }
        return ans;
    }
};